{
    "eula": "",
    "vendor": "YourVendor",
    "license": "",
    "id": "com.yourvendor.averagegauge",
    "version": "1.0.0",
    "supportsMobile": true,
    "name": "AverageGauge",
    "newInstancePrefix": "AverageGauge",
    "description": "A custom gauge widget that calculates and displays the average as a percentage.",
    "webcomponents": [
      {
        "kind": "main",
        "tag": "com-yourvendor-averagegauge",
        "url": "https://thomasgeier03.github.io/CustomWidgets/Copilot-Test-Gauge/main.js",
        "integrity": "",
        "ignoreIntegrity": true
      },
      {
        "kind": "styling",
        "tag": "com-yourvendor-averagegauge-styling",
        "url": "https://thomasgeier03.github.io/CustomWidgets/Copilot-Test-Gauge/styling.js",
        "integrity": "",
        "ignoreIntegrity": true
      }
    ],
    "properties": {
      "width": {
        "type": "integer",
        "default": 600
      },
      "height": {
        "type": "integer",
        "default": 320
      },
      "data": {
        "type": "number[]",
        "default": []
      }
    },
    "events": {},
    "methods": {
      "setData": {
        "description": "Set the data for the gauge.",
        "parameters": [
          {
            "name": "data",
            "type": "integer",
            "description": "The data to set."
          }
        ],
        "body": "this.data = data; this.updateGauge();"
      }
    },
    "dataBindings": {
      "dataBinding": {
        "feeds": [
          {
            "id": "dimensions",
            "description": "Dimensions",
            "type": "dimension"
          },
          {
            "id": "measures",
            "description": "Measures",
            "type": "mainStructureMember"
          }
        ]
      }
    }
  }