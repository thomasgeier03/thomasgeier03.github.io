{
    "eula": "",
    "vendor": "SAP Italia Presales Team",
    "license": "",
    "id": "jscharting.gauge",
    "version": "1.0.0",
    "name": "JSCharting Gauge V2",
    "icon": "data:image/jpeg;base64,",
    "newInstancePrefix": "jscharting_gauge2",
    "description": "Gauge chart based on JSCharting libs",
    "supportsMobile": true,
    "webcomponents": [
        {
            "kind": "main",
            "tag": "it-presales-sac-cw-jscharting-gauge",
            "url": "https://thomasgeier03.github.io/CustomWidgets/Test/jscharting-gauge.js",
           
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
          "kind": "styling",
          "tag": "it-presales-sac-cw-jscharting-gauge-aps",
          "url": "https://thomasgeier03.github.io/CustomWidgets/Test/jscharting-gauge-aps.js",
          
          "ignoreIntegrity": true,
          "integrity": ""
        }
    ],
    "properties": {
        "title": {
            "type": "string",
            "description": "Title of the Gauge chart",
            "default": "Chart title"
        }
    },
    "dataBindings": {
        "myDataSource": {
            "feeds": [
                {
                    "id": "dimensions",
                    "description": "Dimensions",
                    "type": "dimension"
                },
                {
                    "id": "measures",
                    "description": "Measures",
                    "type": "mainStructureMember"
                }
            ]
        }
    },
    "methods": {
        "getTitle": {
            "returnType": "string",
            "description": "The title of the Gauge chart",
            "parameters": [],
            "body": "return this.title;"
        },
        "setTitle": {
            "description": "Set the title of the Gauge chart",
            "parameters": [
                {
                    "name": "title",
                    "type": "string",
                    "description": "The new Title"
                }
            ],
            "body": "this.title = title;"
        },
        "getDataSource": {
            "parameters": [],
            "returnType": "DataSource"
        },
        "getDataBinding": {
            "parameters": [],
            "returnType": "DataBinding"
        },
        "getDimensions": {
            "description": "Get all the Dimensions available in bound DataSource",
            "parameters": [],
            "returnType": "DimensionInfo[]"
        },
        "getMeasures": {
            "description": "Get all the Measures available in bound DataSource",
            "parameters": [],
            "returnType": "MeasureInfo[]"
        },
        "addMeasure": {
            "description": "Add a new Measure to DataBinding",
            "parameters": [
                {
                    "name": "measureId",
                    "type": "string",
                    "description": "ID of the Measure to add"
                }
            ]
        },
        "addDimension": {
            "description": "Add a new Dimension to DataBinding",
            "parameters": [
                {
                    "name": "dimensionId",
                    "type": "string",
                    "description": "ID of the Dimension to add"
                }
            ]
        },
        "removeAllMeasures": {
            "description": "Remove all measures configured in DataBinding"
        },
        "removeAllDimensions": {
            "description": "Remove all dimensions configured in DataBinding"
        }
    },
    "events": {}
}